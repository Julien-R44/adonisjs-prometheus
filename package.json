{
  "name": "@julr/adonisjs-prometheus",
  "type": "module",
  "version": "0.0.8",
  "packageManager": "pnpm@8.3.1",
  "description": "Prometheus wrapper for AdonisJS",
  "author": "Julien Ripouteau <julien@ripouteau.com>",
  "license": "MIT",
  "homepage": "https://github.com/Julien-R44/adonis5-prometheus#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Julien-R44/adonis5-prometheus.git"
  },
  "bugs": {
    "url": "https://github.com/Julien-R44/adonis5-prometheus/issues"
  },
  "keywords": [
    "adonisjs",
    "adonis",
    "prometheus",
    "adonis-prometheus",
    "monitoring",
    "grafana"
  ],
  "exports": {
    ".": "./build/index.js",
    "./types": "./build/src/types.js"
  },
  "main": "build/index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "c8 npm run quick:test",
    "clean": "del-cli build",
    "copy:templates": "copyfiles \"stubs/**/*.stub\" build",
    "compile": "npm run lint && npm run clean && tsc && npm run copy:templates",
    "build": "npm run compile",
    "release": "np",
    "version": "npm run build",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "sync-labels": "github-label-sync --labels .github/labels.json adonisjs/cors",
    "quick:test": "node --loader=ts-node/esm bin/test.ts"
  },
  "peerDependencies": {
    "@adonisjs/assembler": "6.1.3-25",
    "@adonisjs/core": "^6.1.1-0"
  },
  "dependencies": {
    "@poppinss/utils": "^6.5.1",
    "prom-client": "^15.0.0"
  },
  "devDependencies": {
    "@adonisjs/assembler": "6.1.3-25",
    "@adonisjs/core": "^6.1.5-31",
    "@japa/api-client": "^2.0.1",
    "@japa/assert": "^2.0.1",
    "@japa/file-system": "^2.0.1",
    "@japa/runner": "^3.0.5",
    "@julr/eslint-config": "^1.0.0",
    "@swc/core": "1.3.56",
    "@types/node": "^20.8.10",
    "bumpp": "^9.2.0",
    "c8": "^8.0.1",
    "copyfiles": "^2.4.1",
    "defu": "^6.1.3",
    "del-cli": "^5.1.0",
    "eslint": "^8.53.0",
    "github-label-sync": "^2.3.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/core",
        "openapi-types"
      ]
    }
  },
  "eslintConfig": {
    "ignorePatterns": [
      "build"
    ],
    "extends": "@julr",
    "rules": {
      "@typescript-eslint/no-var-requires": "off"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  }
}
